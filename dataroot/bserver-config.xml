<!-- BServer -->
<bserver-config>
	<global>
		<encoding>UTF-8</encoding>
		<timeConsumingWarningMills>1500</timeConsumingWarningMills>
	</global>

	<!--工具类-->
	<preLoadClasses>
		<preLoadClass>kafeihu.zk.bserver.manager.ModuleManager</preLoadClass>
		<preLoadClass>kafeihu.zk.bserver.manager.LoggerManager</preLoadClass>
    	<preLoadClass>kafeihu.zk.bserver.manager.DBConnectionPoolManager</preLoadClass>
    	<preLoadClass>kafeihu.zk.bserver.manager.ContextManager</preLoadClass>
		<preLoadClass>kafeihu.zk.bserver.manager.ProcManager</preLoadClass>
		<preLoadClass>kafeihu.zk.bserver.manager.ExecutorManager</preLoadClass>
		<preLoadClass>kafeihu.zk.bserver.manager.ObjectPoolManager</preLoadClass>
	</preLoadClasses>


	<!--服务模块，socket，监控模块monitor，暂时没实现-->
	<serviceManagers>
		<serviceManager>
			<name>SocketServerManager</name>
			<class>kafeihu.zk.bserver.socket.SocketServerManager</class>
			<description>SocketServer管理</description>
		</serviceManager>
		<serviceManager>
			<name>ScheduleManager</name>
			<class>kafeihu.zk.bserver.schedule.ScheduleManager</class>
			<description>定时任务管理</description>
		</serviceManager>
		<serviceManager>
			<name>MonitorManager</name>
			<class>kafeihu.zk.bserver.monitor.MonitorManager</class>
			<description>监控模块</description>
		</serviceManager>
    </serviceManagers>

</bserver-config>