<!-- monitor监控模块配置
clientIPControl 允许固定ip访问
port 监控模块的端口
prid 监控模块的对外请求
-->
<monitor-config>
    <clientIPControl>
        <allow>Y</allow>
        <equal>127.0.0.1</equal>
    </clientIPControl>
    <port>9999</port>
    <prid>
        <id>GetStatistics</id>
        <impl>kafeihu.zk.bserver.monitor.proc.GetStatistics</impl>
        <desc></desc>
    </prid>
    <prid>
        <id>GetModuleName</id>
        <impl>kafeihu.zk.bserver.monitor.proc.GetModuleName</impl>
        <desc></desc>
    </prid>
    <prid>
        <id>QuerySysLogData</id>
        <impl>kafeihu.zk.bserver.monitor.proc.QuerySysLogData</impl>
        <MaxQuerySpanHours>24</MaxQuerySpanHours>
        <MaxLogDataLineNum>10000</MaxLogDataLineNum>
        <SendBatchNum>1000</SendBatchNum>
        <desc></desc>
    </prid>
</monitor-config>