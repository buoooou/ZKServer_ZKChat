<!-- Socket配置文件
客户端请求分三次处理
接受请求
处理请求
响应请求
 -->
<socketserver-config>
	<socketServer>
			<alias>SocketServer-NPS</alias>
			<serverImpl>kafeihu.zk.bserver.service.socket.BSocketServer</serverImpl>
			<serverSocket>
				<port>8010</port>
				<clientSocketTimeoutMills>15000</clientSocketTimeoutMills>
				<options>
				</options>
			</serverSocket>
			<threadPool>
				<maxPoolSize>200</maxPoolSize>
				<corePoolSize>20</corePoolSize>
				<workQueueSize>0</workQueueSize>
				<keepAliveTimeSeconds>60</keepAliveTimeSeconds>
				<rejectedExecutionHandler>kafeihu.zk.bserver.service.socket.handler.DefaultSocketRejectedExecutionHandler</rejectedExecutionHandler>
			</threadPool>
			<socketWorker>
				<requestHandler>
					<handlerImpl>kafeihu.zk.bserver.service.socket.handler.RecvRequestDataHandler</handlerImpl>
				</requestHandler>
				<requestHandler>
					<handlerImpl>kafeihu.zk.bserver.service.socket.handler.DoRequestProcHandler</handlerImpl>
				</requestHandler>
				<requestHandler>
					<handlerImpl>kafeihu.zk.bserver.service.socket.handler.SendResponseDataHandler</handlerImpl>
				</requestHandler>
				<exceptionHandler>
					<handlerImpl>kafeihu.zk.bserver.service.socket.handler.DefaultSocketExceptionHandler</handlerImpl>
				</exceptionHandler>
			</socketWorker>
			<statistics>
				<spanHours>1</spanHours>
				<freqUnit>S</freqUnit>
			</statistics>
	</socketServer>
</socketserver-config>	
